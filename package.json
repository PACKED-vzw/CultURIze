{
    "name": "culturizeapp",
    "version": "1.2.0",
    "description": "The Culturize Desktop Application",
    "main": "./dist/main.js",
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "start": "npm run build && electron ./dist/main.js",
        "package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --icon=static/assets/icon.ico --prune=true --out=release-builds",
        "package-win64": "electron-packager . --overwrite --asar --platform=win32 --arch=x64 --icon=static/assets/icon.ico --prune=true --out=release-builds",
        "package-lin": "electron-packager . --overwrite --asar --platform=linux --arch=x64 --icon=static/assets/icon.ico --prune=true --out=release-builds",
        "package-mac": "electron-packager . --overwrite --asar --platform=darwin --arch=x64 --icon=static/assets/icon.icns --prune=true --out=release-builds",
        "test": "cross-env TS_NODE_FILES=true mocha --exit --require ts-node/register --colors test/**/*.ts"
    },
    "repository": "https://github.com/packed-vzw/Culturize",
    "keywords": [
        "Electron",
        "resolver",
        "start",
        "tutorial",
        "demo",
        "typescript"
    ],
    "author": "oSoC18 Resolver Team; Deuze, Douwe De Bock and PACKED",
    "license": "MIT",
    "devDependencies": {
        "@types/chai": "^4.2.12",
        "@types/mocha": "^8.0.3",
        "@types/sinon": "^9.0.5",
        "chai": "^4.2.0",
        "cross-env": "^7.0.2",
        "dirty-chai": "^2.0.1",
        "electron": "^24.1.2",
        "electron-packager": "^17.1.1",
        "lodash": ">=4.17.19",
        "mocha": "^10.2.0",
        "sinon": "^4.1.2",
        "ts-mock-imports": "^1.3.0",
        "ts-node": "^10.9.1",
        "ts-sinon": "^2.0.1",
        "tslint": "^5.20.1",
        "typescript": "^4.1.6"
    },
    "dependencies": {
        "@electron/remote": "^2.0.9",
        "@octokit/rest": "^16.43.2",
        "@octokit/types": "^5.5.0",
        "@types/git-url-parse": "^9.0.0",
        "@types/node": "^18.11.9",
        "@types/valid-url": "^1.0.3",
        "csv-parse": "^4.12.0",
        "electron-log": "^3.0.9",
        "git-url-parse": "^13.1.0",
        "got": "^11.8.5",
        "is-github-url": "^1.2.2",
        "rimraf": "^2.7.1",
        "shelljs": "^0.8.4",
        "simple-git": "^3.17.0",
        "valid-url": "^1.0.9"
    }
}
